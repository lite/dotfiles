[user]
	name = lite
	email = litekok@gmail.com
[core]
	excludesfile = /Users/dli/.gitignore
	ignorecase = false
	quotepath = false
[color]
	status = auto
	diff = auto
	branch = auto
[branch "master"]
	merge = master
[alias]
    lg = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --
    vimdiff = difftool -y -t vimdiff -R
[push]
	default = current
[pager]
    diff =
